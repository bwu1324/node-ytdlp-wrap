{
	"name": "node-ytdlp-wrap",
	"version": "1.0.1",
	"description": "A basic yt-dlp wrapper for node projects. Installs the correct binary of yt-dlp for the current platform. Supports Linux, Windows, and MacOS/X.",
	"main": "./lib/index.js",
	"scripts": {
		"lint": "eslint ./**/*.ts",
		"build": "tsc -b",
		"prepare": "npm run build && node ./lib/install"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/bwu1324/node-yt-dlp.git"
	},
	"keywords": [
		"yt-dlp",
		"youtube",
		"nodejs",
		"installer",
		"binary"
	],
	"author": "Bwu1324",
	"license": "GPL-3.0",
	"bugs": {
		"url": "https://github.com/bwu1324/node-yt-dlp/issues"
	},
	"homepage": "https://github.com/bwu1324/node-yt-dlp#readme",
	"devDependencies": {
		"@types/follow-redirects": "^1.14.1",
		"@types/node": "^17.0.24",
		"@typescript-eslint/eslint-plugin": "^5.19.0",
		"eslint": "^8.13.0",
		"typescript": "^4.6.3"
	},
	"dependencies": {
		"follow-redirects": "^1.14.9"
	}
}
